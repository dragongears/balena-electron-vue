{
  "name": "balena-electron-vue",
  "version": "0.1.0",
  "description": "Base Balena Electron Vue app",
  "author": "Art Dahm <art@dahm.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:web": "vue-cli-service build src/web.js",
    "lint": "vue-cli-service lint",
    "serve:electron": "vue-cli-service electron:serve",
    "build:electron": "vue-cli-service electron:build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "build:pi": "vue-cli-service electron:build --legacy --armv7l"
  },
  "main": "background.js",
  "dependencies": {
    "lockfile": "^1.0.4",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "electron": "^3.0.5",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-electron-builder": "^1.1.2",
    "vue-template-compiler": "^2.5.21"
  }
}
